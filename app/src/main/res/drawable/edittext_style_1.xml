<!-- 形状 -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <solid android:color="#F6E3F2" />
    <!-- 边框 -->
    <stroke
        android:width="2dip"
        android:color="#F6E3F2" />
    <!-- 内填充颜色 -->
    <padding
        android:bottom="7dp"
        android:left="10dp"
        android:right="10dp"
        android:top="7dp" />
    <!-- 圆角 -->
    <corners android:radius="16dp" />

</shape>
