<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doiry.baoxiaobao">
    <uses-permission android:name="android.permission.INTERNET"/>
    <!--接入wifi状态-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- SD卡权限 -->
    <!--读-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!--写-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Baoxiaobao">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Baoxiaobao.NoActionBar"
            android:networkSecurityConfig="@xml/network_security_config">
        </activity>
        <activity android:name=".ui.loginUi.RegisterActivity" android:exported="true"> </activity>
        <activity android:name=".ui.CommitWorksheetActivity" android:exported="true"> </activity>
        <activity android:name=".ui.loginUi.LoginActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>